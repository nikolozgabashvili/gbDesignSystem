<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ButtonView">
        <attr name="text" format="string" />
        <attr name="loading" format="boolean" />
        <attr name="btnEnabled" format="boolean" />
        <attr name="btnType" format="enum">
            <enum name="primary" value="0" />
            <enum name="secondary" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="RatingBar">
        <attr name="maxRating" format="integer" />
        <attr name="rating" format="float" />
        <attr name="starSize" format="dimension" />
        <attr name="starSpacing" format="dimension" />
        <attr name="filledColor" format="color" />
        <attr name="unfilledColor" format="color" />
        <attr name="isIndicator" format="boolean" />
        <attr name="cornerRadius" format="dimension" />
    </declare-styleable>

</resources>